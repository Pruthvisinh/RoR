<p id="notice"><%= notice %></p>

<h1>Stds</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Class teacher</th>
      <th>Division</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stds.each do |std| %>
      <tr>
        <td><%= std.name %></td>
        <td><%= std.class_teacher %></td>
        <td><%= std.division %></td>
        <td><%= link_to 'Show', std %></td>
        <td><%= link_to 'Edit', edit_std_path(std) %></td>
        <td><%= link_to 'Destroy', std, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Std', new_std_path %>
